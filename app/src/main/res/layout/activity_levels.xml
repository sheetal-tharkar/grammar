<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    tools:context="com.example.priyamagar.hello.Levels">

    <Button
        android:id="@+id/button17"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignEnd="@+id/level2"
        android:layout_alignRight="@+id/level2"
        android:layout_below="@+id/level1"
        android:layout_marginTop="21dp"
        android:background="@drawable/button"
        android:text="level5"
        android:textSize="10dp" />

    <Button
        android:id="@+id/button15"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignBottom="@+id/button16"
        android:layout_alignLeft="@+id/button9"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignStart="@+id/button9"
        android:background="@drawable/button"
        android:text="level12"
        android:textSize="10dp" />

    <Button
        android:id="@+id/button14"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="29dp"
        android:background="@drawable/button"
        android:text="level10"
        android:textSize="10dp" />

    <Button
        android:id="@+id/button16"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignBottom="@+id/button14"
        android:layout_alignLeft="@+id/button11"
        android:layout_alignStart="@+id/button11"
        android:background="@drawable/button"
        android:text="level11"
        android:textSize="10dp" />

    <Button
        android:id="@+id/button13"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignEnd="@+id/level1"
        android:layout_alignRight="@+id/level1"
        android:layout_below="@+id/level1"
        android:layout_marginTop="21dp"
        android:background="@drawable/button"
        android:text="level4"
        android:textSize="10dp" />

    <Button
        android:id="@+id/button12"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/level1"
        android:layout_marginTop="21dp"
        android:background="@drawable/button"
        android:text="level6"
        android:textSize="10dp" />

    <Button
        android:id="@+id/button11"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignBottom="@+id/button10"
        android:layout_alignLeft="@+id/level2"
        android:layout_alignStart="@+id/level2"
        android:background="@drawable/button"
        android:text="level8"
        android:textSize="10dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button12"
        android:layout_marginTop="29dp"
        android:background="@drawable/button"
        android:text="level7"
        android:textSize="10dp" />

    <Button
        android:id="@+id/level1"
        android:layout_width="100dp"

        android:layout_height="100dp"

        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:autoText="true"
        android:background="@drawable/button"
        android:onClick="Level1"
        android:text="level1"
        android:textSize="10dp"
        android:visibility="visible" />

    <Button
        android:id="@+id/level2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button"
        android:elevation="0dp"
        android:onClick="level2on"
        android:text="level2"
        android:textSize="10dp"
        android:visibility="visible"
        tools:clickable="false" />

    <Button
        android:id="@+id/level3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/button"
        android:onClick="level33"
        android:text="level3"
        android:textSize="10dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignBottom="@+id/button11"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/button"
        android:text="level9"
        android:textSize="10dp" />

</RelativeLayout>
